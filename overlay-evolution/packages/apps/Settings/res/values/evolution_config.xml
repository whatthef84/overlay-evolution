<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Device specific doze package -->
    <string name="config_customDozePackage" translatable="false">com.moto.actions/com.moto.actions.DozeSettingsActivity</string>

    <!-- Whether to show min refresh rate in display settings -->
    <bool name="config_show_min_refresh_rate_switch">true</bool>

    <!-- Whether to show min/max refresh rate in display settings -->
    <bool name="config_show_refresh_rate_controls">true</bool>
    <bool name="config_supports_dynamic_refresh_rate_controls">true</bool>

    <!-- Smart Charging -->
    <bool name="config_supportSmartCharging">true</bool>
     
    <!-- Indicates whether device has a side mounted fingerprint sensor,
         but a dedicated one, i.e. not embedded in the power button. -->
    <bool name="config_is_side_fps" translatable="false">true</bool>
</resources>
